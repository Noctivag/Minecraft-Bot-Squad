{
  "name": "minecraft-bot-squad",
  "version": "3.0.0",
  "description": "Professional Minecraft Bot Squad - Launchable desktop application with settings management and real-time dashboard",
  "main": "app/main.js",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "minecraft",
    "bot",
    "mineflayer",
    "automation",
    "autonomous",
    "plugin",
    "ai",
    "redstone",
    "building",
    "ender-dragon",
    "game-completion",
    "electron",
    "desktop-app",
    "gui"
  ],
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "legacy": "node plugin.js",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux"
  },
  "dependencies": {
    "mineflayer": "^4.20.1",
    "mineflayer-pathfinder": "^2.7.0",
    "minecraft-data": "^3.67.0",
    "vec3": "^0.1.8"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.minecraft.botsquad",
    "productName": "Minecraft Bot Squad",
    "directories": {
      "output": "dist"
    },
    "files": [
      "app/**/*",
      "src/**/*",
      "plugin.js",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "app/assets/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "app/assets/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "app/assets/icon.png",
      "category": "Game"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Noctivag/Minecraft-Bot-Squad"
  },
  "bugs": {
    "url": "https://github.com/Noctivag/Minecraft-Bot-Squad/issues"
  }
}
